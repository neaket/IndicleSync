{
	"manifest_version": 2, 
	"name": "IndicleSync",
	"description": "A simple way to share links amongst all of your devices.",
	"version": "0.0.0.2",
	"permissions": [
		"tabs",
		"storage"		
	],
	"content_security_policy": "script-src 'self' https://cdn.firebase.com https://auth.firebase.com https://*.firebaseIO.com; object-src 'self'",
	"options_page": "options.html",
	"browser_action": {
		"default_icon": "icon_dev_19.png",
		"default_popup": "popup.html"
	}
}